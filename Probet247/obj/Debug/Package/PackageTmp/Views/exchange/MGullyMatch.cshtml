@section Scripts{

    <script>
        filter_menu();
        function filter_menu() {
            $("#main_home").addClass("menu-active");
        }

        //Start Cricket
        $(function () {
            getAllMessages();
        })

        function getAllMessages() {
            $.ajax({
                url: '/exchange/GetMessages1MView1',
                contentType: 'application/html ; charset:utf-8',
                type: 'GET',
                dataType: 'html',
                success: function (result) {
                    var a2 = JSON.parse(result);
                    for (i = 0; i < a2.length; i++) {
                        var mplay = a2[i].play;
                        var mtplay = "offplay";
                        if (mplay == "true") {
                            mtplay = "inplay";
                        }
                        var secmy = '<div class="match-odds-row__title" style="border-bottom:1px solid lightgray;">'+
                                        '<div class="match-odds-row__inplay-flag"><img style="height:22px;" src="/Content/rooms/'+mtplay+'.png" /></div > '+
                                        '<div class="match-odds-row__start-date">'+ a2[i].MatchTime +'<br>'+ a2[i].MatchDate +' </div> '+
                                        '<div class="match-odds-row__start-fancy" style="margin-left:10px;display:none;"><img style="height:40px;" src="/Content/rooms/b1.png" /></div>'+
                                        '<div class="match-odds-row__start-fancy" style="display:none;"><img style="height:40px;margin-left: -15px;" src="/Content/rooms/f1.png" /></div>'+
                                        '<a class="match-odds-row__event-name" style="color:var(--color-main);" href="/exchange/MGully/' + a2[i].EventCode + '">'+ a2[i].Runnername +'</a>'+
                                        '<div class="match-odds-row__start-bookfancy" style="margin-left:10px"><img style="height:40px;" src="/Content/rooms/b1.png" /></div>'+
                                        '<div class="match-odds-row__start-bookfancy"><img style="height:40px;margin-left: -15px;" src="/Content/rooms/f1.png" /></div>'+
                                    '</div>';

                        $('#match_list_div').append(secmy);
                    }
                }
            });
        }
        // End Cricket
    </script>

}