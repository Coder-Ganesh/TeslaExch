@if (Session["UserName"] == null)
{
    { Response.Redirect("~/exchange/Login"); }
}
@{
    ViewBag.Title = "FancyBook";
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")


    <title>FancyBook</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="SHORTCUT ICON" href="~/Content/images/favicon.ico" />
    <link rel="bookmark" href="~/Content/images/favicon.ico" />
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
    <script src="http://ajax.googleapis.com/ajax/libs/jqueryui/1.10.4/jquery-ui.min.js"></script>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link href="~/Content/login.css" rel="stylesheet" />

</head>



<body class="sso-content  betfair layout-page-mobile  ssc-footer jur-international GB  sso-mobile">
    <div id="content-login">
        <div id="header-placeholder">
            <div class="centralWrapper">
                <div class="mod-header mod-header-header yui3-header-content">
                    <a href="/exchange/Mindex"><div class="header-image is-back-visible"></div></a>
                </div>
            </div>
        </div>


        @RenderBody()

        @Scripts.Render("~/bundles/jquery")
        @Scripts.Render("~/bundles/bootstrap")
        @RenderSection("scripts", required: false)
    </div>
</body>
</html>



