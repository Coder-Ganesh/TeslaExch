<script>



      MatchedClientBetRC11();
    function MatchedClientBetRC11() {
        $.ajax({
            type: "GET",
            url: "/SuperAdmin/UClientList",
            dataType: "html",
            data: {mdlid :@ViewBag.DLIDN  },
            success: function (data) {
                var a2 = JSON.parse(data);
                for (i = 0; i < a2.length; i++) {
                    var myvar = ' <a href="/SuperAdmin/transactionHistory?md_id='+a2[i].md_id+'&dist_id=' + a2[i].dl_id + '&uid='+a2[i].id+'">' + a2[i].username + '</a>';
                    $('#userlist111').append(myvar);
                }
            }
        });
    }



</script>

