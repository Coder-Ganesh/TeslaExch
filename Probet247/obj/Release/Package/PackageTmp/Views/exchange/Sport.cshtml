    function QuickLiksAllGetr() {
        $.ajax({
            url: '/Other/QuickLinks',
            contentType: 'application/html ; charset:utf-8',
            type: 'post',
            dataType: 'html',
            success: (function (result) {
                var a1 = JSON.parse(result);
                var a5 = JSON.parse(a1);
                var a2 = a5.quicklinks;
                for (var i = 0; i < a2.length; i++) {
                    if (a2[i].eventTypeId == "1" || a2[i].eventTypeId == "2" || a2[i].eventTypeId == "4") {
                        var vjkk = a2[i].url;
                        var link = "";
                        var Evid = a2[i].eventTypeId;
                        if (vjkk == ".") {

                            var quickLinkById = a2[i].quickLinkById.id;
                            link = "/exchange/Market/" + a2[i].quickLinkById.id;
                        }
                        else {
                            var competition = vjkk.replace("https://www.betfair.com/exchange/football/competition/", "");
                            competition = competition.replace("https://www.betfair.com/exchange/cricket/competition/", "");
                            competition = competition.replace("https://www.betfair.com/exchange/tennis/competition/", "");

                            link = '@Url.Action("Index", "exchange", new { sportid = "-1" , leagueid = "-2" })';
                            link = link.replace("-1", Evid);
                            link = link.replace("-2", competition);
                        }

                        var myvar = '<li class="link animate-repeat">' +
