<script>
    function addZero(i) {
        if (i < 10) {
            i = "0" + i;
        }
        return i;
    }
    function getdatetime(datetimeget) {
        //var x1 = a2[i].MatchTime;
        var MatchTime = "Inplay";
        var x = new Date(datetimeget);
        var hour = addZero(x.getHours());
        var minutes = addZero(x.getMinutes());
        var months = ["Jan", "Feb", "Mar", "Apr", "May", "Jun", "Jul", "Aug", "Sep", "Oct", "Nov", "Dec"];
        var weekday = ["sun", "Mon", "Tue", "Wed", "Thur", "Fri", "Sat"];
        var day = weekday[x.getDay()];
        var month = months[x.getMonth()];
        var year = x.getFullYear();
        var defaultVal = x.defaultValue;
        var currentVal = x.value;
        var date1Timestamp = Date.parse(x) / 1000;
        var y = new Date();
        var ey = new Date(y.toDateString());
        var date2Timestamp = Date.parse(y) / 1000;
        var date3Timestamp = Date.parse(ey) / 1000;
        var difference = date1Timestamp - date2Timestamp;
        var difference1 = date1Timestamp - date3Timestamp;
        if (difference < 0) {
            MatchTime = "Inplay";
        }
        else if (difference > 86400) {
            if (difference >= 86400 && difference < 172800) {
                MatchTime = day + " " + hour + ":" + minutes;
            }
            else {
                MatchTime = month + " " + x.getDate() + " " + hour + ":" + minutes;
            }
        }
        else {
            if (difference1 < 86400 && difference1 > 900) {
                MatchTime = "Today " + hour + ":" + minutes;
            }
            else if (difference1 >= 86400 && difference1 < 172800) {
                MatchTime = day + " " + hour + ":" + minutes;
            }
            else if (difference1 > 0 && difference1 < 900) {
                left_time = parseInt(difference1 / 60);
                MatchTime = "Starting in " + left_time + "'";
            }
            else if (difference1 < 0) {
                MatchTime = "Inplay";
            }
            else {
                MatchTime = month + " " + x.getDate() + " " + hour + ":" + minutes;
            }
        }
        return MatchTime;

    }
</script>
