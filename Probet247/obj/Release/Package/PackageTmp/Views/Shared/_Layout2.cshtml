    <style>


        ul {
            margin-bottom: 0 !important;
            padding: 0 !important;
        }

        li {
            list-style: none !important;
        }

        p {
            margin-bottom: 0 !important;
        }

        .main {
            min-height: calc(100vh - 159px) !important;
        }

        .header {
            background-color: var(--theme1-bg) !important;
            color: var(--primary-color) !important;
        }

            .header a {
                color: var(--primary-color) !important;
            }

        .header-bottom {
            background-color: var(--theme2-bg) !important;
            margin-top: 10px !important;
            padding: 9px 0 !important;
        }

            .header-bottom li a {
                color: var(--secondary-color) !important;
                text-transform: uppercase !important;
                padding: 3px 15px !important;
                font-size: 14px !important;
                display: inline-block !important;
                vertical-align: middle !important;
                a -webkit-transform: translateZ(0) !important;
                transform: translateZ(0) !important;
                box-shadow: 0 0 1px transparent !important;
                -webkit-backface-visibility: hidden !important;
                backface-visibility: hidden !important;
                -moz-osx-font-smoothing: grayscale !important;
                position: relative !important;
                overflow: hidden !important;
                text-align: center !important;
            }

        .header-top {
            height: 60px !important;
            margin-top: 10px !important;
        }

            .header-top ul {
                padding-top: 10px !important;
            }

        .logo img {
            width: auto !important;
            height: 65px !important;
        }

        .logo-login {
            max-height: 65px !important;
            max-width: 100% !important;
        }

        .ballance,
        .expose,
        .account {
            margin-left: 15px !important;
            margin-top: 0,
        }

        .account {
            margin-top: 5px,
        }

        .header-bottom nav {
            padding-top: 0 !important;
            padding-bottom: 0 !important;
            padding-left: 0,
        }

        .header-bottom li a:hover {
            text-decoration: none !important;
            background-color: transparent !important;
            color: inherit !important;
        }

        .header-bottom li a:before {
            content: "" !important;
            position: absolute !important;
            z-index: -1 !important;
            left: 50% !important;
            right: 50% !important;
            bottom: 0 !important;
            background: var(--primary-bg) !important;
            height: 2px !important;
            -webkit-transition-property: left, right !important;
            transition-property: left, right !important;
            -webkit-transition-duration: .3s !important;
            transition-duration: .3s !important;
            -webkit-transition-timing-function: ease-out !important;
            transition-timing-function: ease-out !important;
        }

        .header-bottom li a:hover:before,
        .header-bottom li a:focus:before,
        .header-bottom li a:active:before {
            left: 0 !important;
            right: 0 !important;
        }

        .header-bottom li a.active {
            background-color: var(--primary-bg) !important;
            color: var(--secondary-color) !important;
        }

        .account .fa-chevron-down {
            margin-left: 5px !important;
        }

        .account > span {
            cursor: pointer !important;
            position: relative !important;
        }

        .account ul {
            position: absolute !important;
            top: 30px !important;
            right: 0 !important;
            z-index: 1000 !important;
            min-width: 170px !important;
            max-width: 170px !important;
            padding: 5px 0 !important;
            margin: 2px 0 0 !important;
            font-size: 14px !important;
            text-align: left !important;
            border: 1px solid rgba(0, 0, 0, .15) !important;
            border-radius: 4px !important;
            -webkit-box-shadow: 0 6px 12px rgba(0, 0, 0, .175) !important;
            box-shadow: 0 6px 12px rgba(0, 0, 0, .175) !important;
        }

            .account ul li {
                line-height: 50px !important;
            }

                .account ul li a {
                    font-size: 14px !important;
                    text-transform: capitalize !important;
                    padding: 0 8px !important;
                    color: var(--site-color) !important;
                }

            .account ul h6 {
                padding: 0 8px !important;
                color: var(--site-color) !important;
            }
    </style>
